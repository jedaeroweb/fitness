<section id="show-course-category" class="container">
  <div class="row">
    <%= render :partial => 'layouts/notice', :locals => { :errors => @course_category.errors, :notice => notice, :alert => alert } %>
  </div>
  <div class="row">
    <div class="col-12">
      <div class="card">
        <h3 class="card-header"></h3>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-12 col-xl-6">
      <%= link_to t(:link_list), admin_course_categories_path, class: 'btn btn-secondary' %>
    </div>
    <div class="col-12 col-xl-6 text-right">
      <%= link_to t(:edit), edit_admin_course_category_path(@course_category), class: 'btn btn-secondary' %>
      <%= link_to t(:delete), admin_course_category_path(@course_category), class: 'btn btn-danger' %>
    </div>
  </div>
</section>
